<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.goldfinance.jinGC.mapper.FundProductMapper">

	<select id="findFundCodeByType" resultType="string" parameterType="int" >
		SELECT fund_code FROM FUNDPRODUCT WHERE type=#{type} and flag=1
	</select>
	
</mapper>