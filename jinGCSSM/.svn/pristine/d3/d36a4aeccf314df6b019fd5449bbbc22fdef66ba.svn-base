package com.goldfinance.jinGC.poOra;

import java.util.Date;

/**
 * 该类主要对应MF_FundArchives 基金概况表，某些字段是其他表扩展的
 * 目前字段按需取，有需要再加
 * @author liumj
 *
 */
public class FundOverviewExtend {
	private  int  innercode;
	private	String	fundTypeCode	;		   //基金类别代码
	private  String  fundTypeCodeName;  //基金类别代码对应的中文       -----> 类型
	private	Date 	establishmentDate	;	
	private	String	lastestAssetsValue	;	//最新规模    //基金主要财务指标(季报)表 中的 期末基金资产净值
	private	String	lastestShares	;			//最新份额   //基金份额变动表 中的  期末份额（份）
	private	String	manager	;	
	private	String	investAdvisorCode	;	//基金管理人编码
	private 	String  investAdvisorCodeName;  //基金管理人名称
	private	String	trusteeCode	;		//基金托管人编号
	private  String  trusteeCodeName;  //基金托管人名称
	private	String	investTarget	;	
	private	String	riskEvaluation	;				//基金评级表 中的 风险评价
	private	String	riskEvaluationName	;		//基金评级表 中的 风险评价对应的中文       -----> 风险类型
	private	String	investField	;	
	private	String	investOrientation	;	//投资策略；用'基金投资方向' ，后续需确认
	public int getInnercode() {
		return innercode;
	}
	public void setInnercode(int innercode) {
		this.innercode = innercode;
	}
	public String getFundTypeCode() {
		return fundTypeCode;
	}
	public void setFundTypeCode(String fundTypeCode) {
		this.fundTypeCode = fundTypeCode;
	}
	public String getFundTypeCodeName() {
		return fundTypeCodeName;
	}
	public void setFundTypeCodeName(String fundTypeCodeName) {
		this.fundTypeCodeName = fundTypeCodeName;
	}
	public Date getEstablishmentDate() {
		return establishmentDate;
	}
	public void setEstablishmentDate(Date establishmentDate) {
		this.establishmentDate = establishmentDate;
	}
	public String getLastestAssetsValue() {
		return lastestAssetsValue;
	}
	public void setLastestAssetsValue(String lastestAssetsValue) {
		this.lastestAssetsValue = lastestAssetsValue;
	}
	public String getLastestShares() {
		return lastestShares;
	}
	public void setLastestShares(String lastestShares) {
		this.lastestShares = lastestShares;
	}
	public String getManager() {
		return manager;
	}
	public void setManager(String manager) {
		this.manager = manager;
	}
	public String getInvestAdvisorCode() {
		return investAdvisorCode;
	}
	public void setInvestAdvisorCode(String investAdvisorCode) {
		this.investAdvisorCode = investAdvisorCode;
	}
	public String getInvestAdvisorCodeName() {
		return investAdvisorCodeName;
	}
	public void setInvestAdvisorCodeName(String investAdvisorCodeName) {
		this.investAdvisorCodeName = investAdvisorCodeName;
	}
	public String getTrusteeCode() {
		return trusteeCode;
	}
	public void setTrusteeCode(String trusteeCode) {
		this.trusteeCode = trusteeCode;
	}
	public String getTrusteeCodeName() {
		return trusteeCodeName;
	}
	public void setTrusteeCodeName(String trusteeCodeName) {
		this.trusteeCodeName = trusteeCodeName;
	}
	public String getInvestTarget() {
		return investTarget;
	}
	public void setInvestTarget(String investTarget) {
		this.investTarget = investTarget;
	}
	public String getRiskEvaluation() {
		return riskEvaluation;
	}
	public void setRiskEvaluation(String riskEvaluation) {
		this.riskEvaluation = riskEvaluation;
	}
	public String getRiskEvaluationName() {
		return riskEvaluationName;
	}
	public void setRiskEvaluationName(String riskEvaluationName) {
		this.riskEvaluationName = riskEvaluationName;
	}
	public String getInvestField() {
		return investField;
	}
	public void setInvestField(String investField) {
		this.investField = investField;
	}
	public String getInvestOrientation() {
		return investOrientation;
	}
	public void setInvestOrientation(String investOrientation) {
		this.investOrientation = investOrientation;
	}
	@Override
	public String toString() {
		return "FundOverviewExtend [innercode=" + innercode + ", fundTypeCode="
				+ fundTypeCode + ", fundTypeCodeName=" + fundTypeCodeName
				+ ", establishmentDate=" + establishmentDate
				+ ", lastestAssetsValue=" + lastestAssetsValue
				+ ", lastestShares=" + lastestShares + ", manager=" + manager
				+ ", investAdvisorCode=" + investAdvisorCode
				+ ", investAdvisorCodeName=" + investAdvisorCodeName
				+ ", trusteeCode=" + trusteeCode + ", trusteeCodeName="
				+ trusteeCodeName + ", investTarget=" + investTarget
				+ ", riskEvaluation=" + riskEvaluation
				+ ", riskEvaluationName=" + riskEvaluationName
				+ ", investField=" + investField + ", investOrientation="
				+ investOrientation + "]";
	}
	

}
